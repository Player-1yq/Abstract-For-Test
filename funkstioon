#if tehe in["+","-","*","/"]:

print("1.Tekst\n2.Kalkulaator\n3.Ruudu kujund")

menüü_valik = input("Mis valiku teed(1, 2, 3): ")
if menüü_valik == "1":
    print("Viin teksti")
elif menüü_valik == "2":
    print("Viin kalkulaatorisse")
elif menüü_valik == "3":
    print("Viin ruudu kujundisse")


def calculator():
    try:
        a = float(input("Sisestage esimene arv: "))
        b = float(input("Sisestage teiner arv: "))
        op = input("vailge  (+, -, *, /): ")

        if op == "+":
            result = a + b
        elif op == "-":
            result = a - b
        elif op == "*":
            result = a * b
        elif op == "/":
            if b == 0:
                raise ZeroDivisionError
            result = a / b
        else:
            print("Tundmatu operatsioon")
            return

        print("tulemus:", result)

    except ValueError:
        print("Viga: peate sisestama numbrid")
    except ZeroDivisionError:
        print("Viga: jagamine nulliga")
    except Exception as e:
        print("Teine viga:", e)


calculator()


def square():
    try:
        size = int(input("Sisestage ruudu suurus: "))
        for i in range(size):
            print("* " * size)
    except ValueError:
        print("Viga: peate sisestama täisarvu")
